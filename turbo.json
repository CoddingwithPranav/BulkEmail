{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**","dist/**", "build/**", ".vite/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "type-check": {
      "dependsOn": ["^type-check"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": [
    "tsconfig.json",
    "tailwind.config.js",
    "postcss.config.js",
    ".env*"
  ],
  "globalEnv": ["NODE_ENV"],
  "globalPassThroughEnv": ["VITE_*", "API_*"],
  "ui": "tui",
  "concurrency": "50%",
  "remoteCache": {
    "signature": true
  }
}