//  binaryTargets = ["native", "linux-musl"]
// }

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model User {
  id       String  @id @unique @default(cuid())
  username String  @unique
  password String 
  displayName String @default("")
  profileImage String @default("")
}